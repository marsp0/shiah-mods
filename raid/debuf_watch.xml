            <Frame name="$parent11" inherits="SingleDebuffWatchFrame">
                <Anchors><Anchor point="TOPLEFT"><Offset><AbsDimension x="0" y="0" /></Offset></Anchor></Anchors>
                <Scripts>
                    <OnLoad>
                        local texture = _G[self:GetName().."Texture"]
                        texture:SetTexture("Interface/icons/ability_warrior_sunder")
                        texture:SetVertexColor(0.5, 0.5, 0.5)
                        self.time_since_last_update = 0
                        self.spell_id = 11597
                    </OnLoad>
                    <OnUpdate>
                        debuff_watch_update(self)
                    </OnUpdate>
                </Scripts>
